    "@sanity/assist": "^3.0.8",
    "@sanity/cli": "^3.64.2",
    "@sanity/demo": "2.0.0",
    "@sanity/icons": "^3.3.0",
    "@sanity/image-url": "^1.0.2",
    "@sanity/preview-url-secret": "^1.6.19",
    "@sanity/react-loader": "^1.10.3",
    "@sanity/ui": "^2.8.24",
    "@sanity/vision": "^3.64.2",
    "@tailwindcss/typography": "0.5.13",
    "classnames": "2.5.1",
    "date-fns": "3.6.0",
