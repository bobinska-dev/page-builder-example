  "devDependencies": {
    "@types/react": "^18.3.3",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.4",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "postcss": "8.4.38",
    "prettier": "3.3.2",
    "prettier-plugin-packagejson": "^2.5.0",
